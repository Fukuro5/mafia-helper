{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "react-native/no-unused-styles": "error",
    "import/no-unresolved": "error",
    "import/no-extraneous-dependencies": "off",
    "default-param-last": "off"
  }
}
